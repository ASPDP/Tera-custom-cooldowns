<Window x:Class="TCC.Windows.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCC.Windows"
        mc:Ignorable="d" SizeToContent="WidthAndHeight"
        Title="DebugWindow" Height="Auto" Width="Auto">
    <StackPanel>
        <Button Width="70" Height="20" Click="Start">Start</Button>
        <Button Width="70" Height="20" Click="Stop">Stop</Button>
        <Button Width="70" Height="20" Click="Dump">Dump</Button>
    </StackPanel>
</Window>
