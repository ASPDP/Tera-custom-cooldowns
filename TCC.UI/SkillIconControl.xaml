<UserControl x:Class="TCC.UI.SkillIconControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TCC.UI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_Loaded">
    <Grid Width="Auto" Height="Auto" Margin="2">
        <Ellipse Width="50" Height="50" x:Name="icon" Fill="{Binding IconBrush}"/>
        <local:Arc x:Name="arc" Width="59" Height="59" StrokeThickness="4" Stroke="White" EndAngle="150.9">
        </local:Arc>
    </Grid>
</UserControl>
